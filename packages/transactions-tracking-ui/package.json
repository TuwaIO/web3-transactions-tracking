{
  "name": "@tuwa/transactions-tracking-ui",
  "description": "A suite of beautiful, pre-styled React components and a dedicated provider to simplify real-time transaction tracking in your dApp.",
  "version": "0.0.1",
  "author": "Oleksandr Tkach",
  "license": "Apache-2.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    {
      "name": "Oleksandr Tkach",
