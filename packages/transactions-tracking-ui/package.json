  },
  "homepage": "https://github.com/TuwaIO/web3-transactions-tracking",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . && prettier --write .",
    "preview": "vite preview",
    "tsup-start": "tsup src/index.ts --watch",
    "tsup-build": "tsup && pnpm exec postcss ./src/styles/app.css -o ./dist/index.css",
    "ci:publish": "npm run build && npm publish --access=public",
    "ci:publish:beta": "npm run build && npm publish --tag beta --access=public"
  },
  "dependencies": {
    "@bgd-labs/react-web3-icons": "^1.34.0",
    "@gelatonetwork/relay-sdk": "^5.6.0",
