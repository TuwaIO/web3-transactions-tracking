    "@vitejs/plugin-react": "^4.4.1",
    "typescript": "~5.8.3",
    "tailwindcss": "^4.1.11",
    "tsup": "^8.5.0",
    "vite": "^6.3.4",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "autoprefixer": "^10.4.21"
  }
