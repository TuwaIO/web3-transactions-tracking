    "@rainbow-me/rainbowkit": "^2.2.4",
    "@tanstack/react-query": "^5.74.11",
    "@tuwa/evm-transactions-tracking": "workspace:^",
    "@tuwa/web3-transactions-tracking-core": "workspace:^",
    "@wagmi/core": "^2.17.2",
    "dayjs": "^1.11.13",
    "immer": "^10.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-toastify": "^11.0.5",
    "react-modal": "^3.16.3",
    "superjson": "^2.2.2",
    "viem": "^2.28.1",
